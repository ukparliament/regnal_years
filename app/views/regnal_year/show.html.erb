<%= content_tag( 'p', @regnal_year.display_dates ) %>

<%= render :partial => 'previous_next' %>

<%= content_tag( 'p', "A regnal year for #{link_to( @regnal_year.monarch_title, monarch_show_url( :monarch => @regnal_year.monarch_id ) )}.".html_safe ) %>

<% unless @sessions.empty? -%>
	<div id="session">
		<%= content_tag( 'h2', 'Sessions' ) %>
		<table>
			<%= render :partial => 'session/session_table_head' %>
			<tbody>
				<%= render :partial => 'session/session', :collection => @sessions %>
			</tbody>
		</table>
	</div>
<% end -%>